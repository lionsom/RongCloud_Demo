#  pod 'DoraemonKit/WithWeex', '~> 3.0.4', :configurations => ['Debug'] #可选
#  pod 'DoraemonKit/WithDatabase', '~> 3.0.4', :configurations => ['Debug'] #可选
#  pod 'DoraemonKit/WithMLeaksFinder', '3.0.4', :configurations => ['Debug'] #可选
  
end
